{
  "compilerOptions": {
    /* Basic Options */
    "target": "es2017",
    "module": "esnext",

    /* Module Resolution Options */
    "moduleResolution": "node",
    "esModuleInterop": true,

    /* Advanced Options */
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
